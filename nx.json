{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "preview": {
      "dependsOn": [
        "^preview"
      ]
    }
  },
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/rsbuild",
      "options": {
        "buildTargetName": "rsbuild:build",
        "devTargetName": "rsbuild:dev",
        "previewTargetName": "rsbuild:preview",
        "inspectTargetName": "inspect",
        "typecheckTargetName": "typecheck"
      }
    }
  ]
}
